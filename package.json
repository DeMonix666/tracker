{
  "name": "Tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install:pods": "NO_FLIPPER=1 pod install --repo-update --project-directory=./ios",
    "install:pods:noflipper": "NO_FLIPPER=1 pod install --repo-update --project-directory=./ios",
    "clean:ios": "rm -rf ios/build ios/Pods ios/Podfile.lock",
    "clean:android": "cd android && rm -rf build && ./gradlew clean",
    "android": "react-native run-android --appIdSuffix=test",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "eject": "npx react-native eject",
    "test": "jest",
    "android:bundle:debug": "mkdir -p android/app/src/main/assets && rm -rf android/app/build && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:assemble:debug": "cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
